/**
* @brief Klasa-interfejs do obs≈Çugi klonowania
*  Obecnie ta klasa nie jest wykorzysytwana
*/
class Clonable{
public: 
	virtual ~Clonable(){};
	Clonable(){};
	Clonable(const Clonable&){};
	virtual Clonable & operator=(const Clonable&)=0;
	virtual Clonable & Clone()=0;
};
